    "build-dev": "NODE_ENV=development node scripts/build.js",
    "deploy": "aws s3 sync build s3://yup-racing",
    "deploy-staging": "aws s3 sync build s3://yup-racing-staging",
    "release": "yarn config set version-tag-prefix frontend@v &&  yarn config set version-git-message 'frontend@v%s' && yarn version",
